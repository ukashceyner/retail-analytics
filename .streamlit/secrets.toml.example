# ABOUTME: Example Streamlit secrets configuration for local development.
# ABOUTME: Copy to secrets.toml and fill in your Supabase credentials.

[database]
url = "postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres"

[supabase]
url = "https://YOUR_PROJECT_REF.supabase.co"
key = "YOUR_ANON_KEY"
